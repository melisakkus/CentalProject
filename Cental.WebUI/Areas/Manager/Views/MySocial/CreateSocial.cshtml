@model CreateUserSocialDto
@{
    ViewData["Title"] = "Yeni Sosyal Medya Ekle";
    Layout = "~/Areas/Manager/Views/ManagerLayout/Index.cshtml";
}

<form method="post">

    <label>Sosyal Medya Adı</label>
    <input type="text" asp-for="Title" class="form-control"/>
    <br />

    <label>Sosyal Medya Ikonu</label>
    <input type="text" asp-for="Icon" class="form-control" />
    <br />

    <label>Sosyal Medya Url</label>
    <input type="text" asp-for="Url" class="form-control" />
    <br />

    <button class="btn btn-outline-dark">Kaydet</button>
</form>
