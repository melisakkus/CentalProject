
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/UILayout/Index.cshtml";
}

<!-- Carousel Start -->
@await Component.InvokeAsync("_DefaultBannerComponent")
<!-- Carousel End -->
<!-- Features Start -->
@await Component.InvokeAsync("_DefaultFeaturesComponent")
<!-- Features End -->
<!-- About Start -->
@await Component.InvokeAsync("_DefaultAboutComponent")
<!-- About End -->
<!-- Fact Counter -->
@await Component.InvokeAsync("_DefaultFactCounterComponent")
<!-- Fact Counter -->
<!-- Services Start -->
@await Component.InvokeAsync("_DefaultServicesComponent")
<!-- Services End -->
<!-- Car categories Start -->
@await Component.InvokeAsync("_DefaultCarsComponent")
<!-- Car categories End -->
<!-- Car Steps Start -->
@await Component.InvokeAsync("_DefaultProcessComponent")
<!-- Car Steps End -->
<!-- Banner Start -->
<div class="container-fluid banner pb-5 wow zoomInDown" data-wow-delay="0.1s">
    <div class="container pb-5">
        <div class="banner-item rounded">
            <img src="~/Cental-1.0.0/img/banner-1.jpg" class="img-fluid rounded w-100" alt="">
            <div class="banner-content">
                <h2 class="text-primary">Rent Your Car</h2>
                <h1 class="text-white">Interested in Renting?</h1>
                <p class="text-white">Don't hesitate and send us a message.</p>
                <div class="banner-btn">
                    <a href="#" class="btn btn-secondary rounded-pill py-3 px-4 px-md-5 me-2">WhatchApp</a>
                    <a href="#" class="btn btn-primary rounded-pill py-3 px-4 px-md-5 ms-2">Contact Us</a>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Banner End -->
<!-- Team Start -->
@await Component.InvokeAsync("_DefaultManagerComponent");
<!-- Team End -->
<!-- Testimonial Start -->
@await Component.InvokeAsync("_DefaultTestimonialComponent")
<!-- Testimonial End -->
