@model UpdateProcessDto
@{
    ViewData["Title"] = "Süreç Bilgisi Güncelleme ";
    Layout = "~/Areas/Admin/Views/AdminLayout/Layout.cshtml";
}

<form method="post">
    <input type="hidden" asp-for="ProcessId"/>
    <div>
        <label>Başlık</label>
        <input type="text" class="form-control" asp-for="Title" />
    </div>
    <br />

    <div>
        <label>Açıklama</label>
        <input type="text" class="form-control" asp-for="Description" />
    </div>
    <br />

    <div class="text-center">
        <button class="btn btn-outline-primary">Güncelle</button>
    </div>
    <br />
</form>
